{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 * * * *"
    },
    {
      "name": "outputBlob",
      "path": "outcontainer/{datetime}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out",
      "type": "blob"
  }
  ]
}
